
# Site Archiving Toolkit aliases
alias archive="cd /root/site-archiving-toolkit && ./archive.sh"
alias attach="cd /root/site-archiving-toolkit && ./attach.sh"
alias quit-crawlers="cd /root/site-archiving-toolkit && ./quit-crawlers.sh"
alias start-server="cd /root/site-archiving-toolkit && ./start-server.sh"
alias stop-server="cd /root/site-archiving-toolkit && ./stop-server.sh"